#accountProfile
  font-family "source sans pro" "PT Sans"
  a
    color black
    cursor pointer
  p
    font-size 14px
  a
    font-size 14px
  b
    font-size 14px
  .profile-wrapper
    position absolute
    padding-bottom 80px
    padding-top 60px
    margin auto
    left 0
    right 0
    .profile-cell
      margin-bottom 20px
      .profile-header
        font-family "PT Sans"
        font-weight 700
        font-size 18px
        padding-bottom 10px
        margin-right 40px
        margin-bottom 20px
        border-bottom 1px solid #d3d3d3
        button
          font-size 14px       
          border-radius 2px
          background-color rgb(230, 230, 230)
          border none
    #profile
      img
        width 20px
        margin-left 15px
    #giftVoucher
      .refresh-btn
        margin-left 80px
        background url('/assets/images/refresh-btn.png') left center no-repeat
        border none
        padding-left 20px
      input[type="input"]
        width 40%
        height 40px
        margin-right 20px
        background-color #f7f7f7
        border none
      input[type="submit"]
        font-size 18px
        height 40px
        width 80px
        font-family: "PT Sans";
        color white
        font-weight 700
        border none
        border-radius 2px
        background-image -moz-linear-gradient( 90deg, rgb(78,78,78) 0%, rgb(124,124,124) 100%)
        background-image -webkit-linear-gradient( 90deg, rgb(78,78,78) 0%, rgb(124,124,124) 100%)
        background-image -ms-linear-gradient( 90deg, rgb(78,78,78) 0%, rgb(124,124,124) 100%)
        box-shadow 0.157px 2.996px 0px 0px rgba(87, 87, 87, 0.294)  