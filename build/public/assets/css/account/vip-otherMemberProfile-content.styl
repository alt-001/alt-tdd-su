#vipOtherMemberProfileContent
  font-family 'PT Sans'
  padding 20px
  padding-top 10px
  h4
    font-weight 700
    margin-bottom 20px
  .updates
    .lists
      .media-info
        min-height 129px
        display table-cell
        .thumbnail
          padding-top 20px
          margin-right 6px
          border 0
          background-color #e2e2e2
          position relative
          z-index 1
          margin-bottom   10px
          -webkit-border-radius  2px
          -moz-border-radius  2px
          border-radius  2px
          img
            width 55px
            height 55px
            border 1px solid #d3d3d3
        .caption
          text-align center
          .name
            font-weight 700
            font-family 'PT Sans'
            font-size 14px
            margin-bottom 0
            padding-top 0
          .role
            font-weight 100
            font-family 'PT Sans'
            font-size 12px  
        .duration
          text-align right
          color #d3d3d3
          margin-right 10px
          font-family 'Source Sans Pro'
        .connection
          background-image url("/assets/images/pro/icon-connections.png") 
          height 60px
          width 60px
          position absolute
          top 23%
          left -35px
          z-index 1
        .likes
          background-image url("/assets/images/pro/icon-love.png") 
          height 58px
          width 59px
          position absolute
          top 23%
          left -32px
          z-index 1 
        &:nth-child(odd)  
          .thumbnail
            margin-left 0px
        &:nth-child(even) 
          .thumbnail
            margin-right 10px
  .alert-msg
    margin-top 10px
    padding-top 10px
    border-top 1px solid #d3d3d3
    button
      margin-top 20px
      margin-right 10px
      border-radius 2px
      background-color rgb(102, 102, 102)
      font-weight 700
      color white
      letter-spacing .5px
      &.back-btn
        background-color #dfdfdf
        color rgb(102, 102, 102)